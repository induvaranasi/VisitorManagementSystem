<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<Dialog title="Access Card" draggable="true" contentWidth="35rem" resizable="true" class="sapUiSizeCompact" verticalScrolling="true">
		<content>
			<f:SimpleForm id="idSimpleForm" editable="false">
				<f:content>
					<Label text="Visitor Name"></Label>
					<Text text="{oSecurityModel>/CheckInVisitorDetails/visitorName}"></Text>
					<Label text="Email"></Label>
					<Text text="{oSecurityModel>/CheckInVisitorDetails/email}"></Text>
					<Label text="Host Name"></Label>
					<Text text="{oSecurityModel>/CheckInVisitorDetails/hostName}"></Text>
					<Label text="Room Name"></Label>
					<Text text="{oSecurityModel>/CheckInVisitorDetails/roomName}"></Text>
					<Label text="AccessCard No"></Label>
					<Text></Text>
				</f:content>
			</f:SimpleForm>
		</content>
		<Toolbar>
			<ToolbarSpacer></ToolbarSpacer>
			<Button text="Assign" type="Emphasized" press="onAssignAccessCard"/>
		</Toolbar>
	</Dialog>
</core:FragmentDefinition>