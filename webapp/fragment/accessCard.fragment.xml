			<!--		<Text text="{oSecurityModel>/CheckInVisitorDetails/hostName}"></Text>-->
			<!--		<Label text="Room Name"></Label>-->
			<!--		<Text text="{oSecurityModel>/CheckInVisitorDetails/roomName}"></Text>-->
			<!--		<Label text="AccessCard No"></Label>-->
			<!--		<Text></Text>-->
			<!--	</f:content>-->
			<!--</f:SimpleForm>-->
		</content>
		<Toolbar>
			<ToolbarSpacer></ToolbarSpacer>
			<Button text="Assign" type="Emphasized" press="onAssignAccessCard"/>
		</Toolbar>
	</Dialog>
</core:FragmentDefinition>